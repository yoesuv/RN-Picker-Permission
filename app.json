{
  "expo": {
    "name": "RN-Permission-Picker",
    "slug": "RN-Permission-Picker",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#673AB7"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.yoesuv.RNPermissionPicker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to pick image from gallery",
          "cameraPermission": "The app accesses your camera to take photo",
          "microphonePermission": "The app accesses your microphone to record audio"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "The app accesses your current location"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "df5ddfe7-2f19-41dc-85f9-197e504faac4"
      }
    }
  }
}
